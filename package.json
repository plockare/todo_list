{
  "name": "todo_list",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www",
    "dev": "babel-node ./src/bin/www",
    "forever": "forever ./build/bin/www",
    "env-local": "node db local",
    "env-dev": "node db dev",
    "env-test": "node db test",
    "db-clean": "node db clean",
    "test": "NODE_ENV=test node test/test",
    "mocha": "NODE_ENV=test node node_modules/mocha/bin/mocha --compilers js:babel-core/register --colors",
    "eslint": "./node_modules/eslint/bin/eslint.js src/**/*.js",
    "coverage": "npm run eslint && NODE_ENV=test nyc ./node_modules/mocha/bin/_mocha test/files"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bunyan": "^1.8.4",
    "cfg-loader": "0.0.5",
    "cookie-parser": "~1.3.5",
    "dottie": "^1.1.1",
    "express": "~4.13.1",
    "express-validator": "^2.20.8",
    "lodash": "^4.16.6",
    "mysql2": "^1.1.1",
    "sequelize": "^4.0.0-2",
    "sequelize-decorators": "0.0.4",
    "validation": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-bluebird": "^1.0.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-grunt": "^1.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "1.1.0-alpha.1 ",
    "mocha": "^3.0.2",
    "nyc": "^9.0.1",
    "should": "^11.1.0",
    "supertest": "^2.0.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/resources/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "sourceMap": false,
    "instrument": false
  }
}
